@model Make3DProjectUI.Models.ArticleASPModel

@{
    ViewData["Title"] = "Designaler";
}

<h1>Designaler</h1>

<h3>Êtes vous sur de vouloir désignler cet article ?</h3>
<div>
    <h4>ArticleASPModel</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nom)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nom)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomCreateur)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomCreateur)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date_envoi)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date_envoi)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date_modif)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date_modif)
        </dd>
    </dl>
    
    <form asp-action="Designaler">
        <input type="submit" value="Confirmer" class="btn btn-success" /> |
        <a asp-action="Index">Annuler</a>
    </form>
</div>
